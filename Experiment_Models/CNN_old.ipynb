    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"n_features = 3\\n\",\n",
    "    \"def get_segments(df, segment_size, step):\\n\",\n",
    "    \"    segments = []\\n\",\n",
    "    \"    labels = []\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    for i in range(0, df.shape[0] - segment_size, step):\\n\",\n",
    "    \"        axs = df['ax'].values[i: i + segment_size]\\n\",\n",
    "    \"        ays = df['ay'].values[i: i + segment_size]\\n\",\n",
    "    \"        azs = df['az'].values[i: i + segment_size]\\n\",\n",
    "    \"        label = df['weight_class'].iloc[i: i + segment_size].mode()[0]\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        segments.append([axs, ays, azs])\\n\",\n",
    "    \"        labels.append(label)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Reshape the segments\\n\",\n",
    "    \"    reshaped_segments = np.asarray(segments, dtype= np.float32).reshape(-1, segment_size, n_features)\\n\",\n",
    "    \"    labels = np.asarray(labels)\\n\",\n",
